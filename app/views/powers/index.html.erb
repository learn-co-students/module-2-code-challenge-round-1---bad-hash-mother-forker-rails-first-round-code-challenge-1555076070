<h2 align="center" style="color: yellow">All Superpowers</h2>

<ul>
  <% @powers.each do |power| %>
    <li>
      <%= link_to power.name, power_path(power) %>
      <br>
      Description: <%= power.description %>
    </li>
    <br>
  <% end %>
</ul>
<br>
<br>
<p><%= link_to "Click to view current heroines!", heroines_path %></p>